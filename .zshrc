#BASH ALIASES
alias newalias="open ~/.zshrc" #opens this file (so...put this file there!)
alias refresh="source ~/.zshrc" #reload the profile to consume changes to this file"
alias hosts="sudo nano /etc/hosts" #this file always opens read-only
alias sshconfig="open ~/.ssh/config"

#DEVELOPMENT
alias code="open -a Visual\ Studio\ Code"
